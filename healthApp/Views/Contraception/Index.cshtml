@{
}
@model List<healthApp.Models.FamilyPlanning.ContraceptionReminder>

<h2>Contraception Reminders</h2>

<ul>
    @foreach (var reminder in Model)
    {
        <li>
            @Html.DisplayFor(modelItem => reminder.Method) -
            @Html.DisplayFor(modelItem => reminder.NextReminderDate.ToShortDateString())
            <!-- Add other relevant data -->
        </li>
    }
</ul>
